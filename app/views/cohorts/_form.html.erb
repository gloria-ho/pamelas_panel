<%= form_for @cohort do |f| %>
  <div>
    <% f.label :name %> Name: 
    <%= f.text_field :name %>
  </div>

  <div>
    <% f.label :start_date %> Start Date: 
    <%= f.date_field :start_date %>
  </div>

  <div>
    <% f.label :end_date %> Start Date: 
    <%= f.date_field :end_date %>
  </div>

  <div>
    <%= f.submit %>
  </div>
<% end %>